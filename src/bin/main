/* vim: set ft=cpp: */

#include <prelude>
#include <math>
#include <io>

auto main(i32 argc, i8* argv[]) -> i32
{
	out << "Hello, world!" << endl;

	f32 f = 420.69;

	out << "Here is a floating point number: " << f << endl; 

	out << "strlen(\"1234\") = " << strlen("1234") << endl;

	out << "Please input chars:" << endl;

	string data;

	in >> data;

	out << "you wrote: " << data << endl;
}
