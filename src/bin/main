/* vim: set ft=cpp: */

#include <prelude>
#include <math>
#include <io>

auto main(i32 argc, i8* argv[]) -> i32
{
	mat4 matrix = identity<f32, 4>();
	
	matrix[2][2] = 3;

	out << "Hello, world!" << endl;

	f32 f = 420.69;

	//for some reason, have to send endl twice for newline
	out << "Here is a floating point number: " << f << endl << endl;

	out << strlen("1234") << endl;

	out << "Please input chars:" << endl;

	string data;

	in >> data;

	out << "you wrote: " << data << endl;
}
